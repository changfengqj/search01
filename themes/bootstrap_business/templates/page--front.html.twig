        <div class="container">

            <!-- #top-content-inside -->
            <div id="top-content-inside" class="clearfix">
                <div class="row">
                    <div class="col-md-12">
                    {{ page.highlighted }}
                    </div>
                </div>
            </div>
            <!-- EOF:#top-content-inside -->

        </div>
    </div>
    <!-- EOF: #top-content -->
    {% endif %}

    <!-- #main-content -->
    <div id="main-content">
			{% if page.front_gonggao %}
                <div id="front_gonggao" class="clearfix">
                    <div class="container">
                        {{ page.front_gonggao }}
